<%= form_for([@video,@comment]) do |f| %>
  

  <div class="field">
    <%= f.label :time_stamp %><br>
    <%= f.text_field :time_stamp %>
  </div>
  <div class="field">
    <%= f.label :comment_text %><br>
    <%= f.text_area :comment_text %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
